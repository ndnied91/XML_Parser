    <rules>
      <override-rule id="400" name="MobileSuppression">
        <condition>
          <when>
            <event event-type="onPageLanding"/>
          </when>
          <and>
            <not>
              <is-service-blocked service="ANY"/>
            </not>
            <is-device-type type="Unsupported"/>
          </and>
        </condition>
        <actions>
          <block-service>
            <services>
              <all/>
            </services>
            <for-visit/>
          </block-service>
          <log>
            <string>*** TC: Chat suppressed. Device not supported.</string>
          </log>
        </actions>
      </override-rule>

      <rule id="1100" name="Wings-setPreProdVA-TestFlag">
        <condition>
          <when>
            <event event-type="onPageLanding"/>
          </when>
          <where>
            <url-match regexp="(.*)(P|p)(r|R)(E|e)(P|p)(R|r)(O|o)(D|d)(v|V)(a|A)"/>
          </where>
        </condition>
        <actions>
          <set-string-var var-id="VA-SpecID">
            <!--Preprod-->
            <string>18000859</string>
          </set-string-var>
          <set-string-var var-id="NinaEnv">
            <string>ninaPreprod</string>
          </set-string-var>
          <log>
            <string>**************TESTING PRE-PROD*****************</string>
          </log>
        </actions>
      </rule>
      <rule id="1101" name="LaunchSurvey_onbeforeclose">
        <condition>
          <when>
            <service-event service-event="onBeforeChatClosed" service-type="ALL"/>
          </when>
          <and>
            <string-compare test="equals">
              <string-variable-ref var-id="assistChatID"/>
              <chat-id/>
            </string-compare>
            <not>
              <boolean-variable-ref var-id="surveyPopped"/>
            </not>
          </and>
        </condition>
        <actions>
          <set-boolean-var var-id="surveyPopped">
            <true/>
          </set-boolean-var>
          <show-survey native-post-chat="true">
            <survey-spec-id>
              <number>2001</number>
            </survey-spec-id>
          </show-survey>
          <log>
            <string>CHAT* Post-Chat Survey Launched</string>
          </log>
        </actions>
      </rule>
      <rule id="1102" name="DIV-Injection">
        <condition>
          <when>
            <event event-type="onPageLanding"/>
          </when>
          <not>
            <exists>
              <js-string-ref js-var-id="document.getElementById('nuance-chat')"/>
            </exists>
          </not>
        </condition>
        <actions>
          <execute-custom-js>
            <![CDATA[
                            if (null == document.getElementById("nuance-chat")){
                                try {
                                    var div = document.createElement("DIV");
                                    div.innerHTML = '<div id="nuance-chat" style="position: fixed; right:50px; bottom: 0px;z-index:9999"></div>';
                                    document.body.appendChild(div.firstChild);
                                } catch (e) {}
                            }
                          ]]>
          </execute-custom-js>
        </actions>
      </rule>

      <rule id="1104" name="set-NinaVars">
        <condition>
          <when>
            <event event-type="onPageLanding"/>
          </when>
          <always/>
        </condition>
        <actions>
          <conditional-action>
            <if>
              <exists>
                <js-function-result>
                  <anonymous-js-function>
                    <![CDATA[
									function(){
                    try {
										  return hb.nuanceData;}
                    catch(er){}
					          return null;
									}
									]]>
                  </anonymous-js-function>
                </js-function-result>
              </exists>
              <do>
                <map-set>
                  <map-variable-ref var-id="automatonDatapass"/>
                  <key>
                    <string>clienttoken</string>
                  </key>
                  <value>
                    <as-string>
                      <js-function-result>
                        <anonymous-js-function>
                          <![CDATA[
                        function(){
                          try {if(hb.nuanceData.Account_ID){return hb.nuanceData.Account_ID;}}
                          catch (err){console.log("***datapass missing: " + err);}
                          return "not_set";
                        }
                        ]]>
                        </anonymous-js-function>
                      </js-function-result>
                    </as-string>
                  </value>
                </map-set>
              </do>
            </if>
            <else>
              <map-set>
                <map-variable-ref var-id="automatonDatapass"/>
                <key>
                  <string>clienttoken</string>
                </key>
                <value>
                  <string>not_set</string>
                </value>
              </map-set>
            </else>
          </conditional-action>
          <map-set>
            <map-variable-ref var-id="automatonDatapass"/>
            <key>
              <string>PageMarker</string>
            </key>
            <value>
              <current-page-marker/>
            </value>
          </map-set>
          <log>
            <string>NinaVars saved</string>
          </log>
          <!--Test Datapass
          <send-data-to-agent>
            <item name="datapass">
              <map-variable-ref var-id="automatonDatapass"/>
            </item>
          </send-data-to-agent>-->
        </actions>
      </rule>
      <rule id="1105" name="agent-datapass-UserLoggedIn">
        <condition>
          <when>
            <event event-type="onPageLanding"/>
          </when>
          <is-service-in-progress service-type="ANY"/>
          <!--    <boolean-variable-ref var-id="liveChat"/>-->
        </condition>
        <actions>
          <conditional-action>
            <if>
              <and>
                <where>
                  <content-group-check cg-id="WNG_OLB"/>
                </where>
                <not>
                  <boolean-variable-ref var-id="NameSent"/>
                </not>
              </and>
              <do>
                <send-data-to-agent>
                  <item name="Authenticated_User">
                    <as-string>
                      <string-variable-ref var-id="userName"/>
                    </as-string>
                  </item>
                </send-data-to-agent>
                <set-boolean-var var-id="NameSent">
                  <true/>
                </set-boolean-var>
                <log>
                  <string>Rule 1105 Fired:data sent</string>
                </log>
              </do>
            </if>
          </conditional-action>
        </actions>
      </rule>
      <rule id="1106" name="agent-datapass-onAgentAssigned">
        <condition>
          <when>
            <event event-type="onAgentAssigned"/>
          </when>
          <not>
            <string-compare test="equals">
              <string-variable-ref var-id="userName"/>
              <string>not_set</string>
            </string-compare>
          </not>
        </condition>
        <actions>
          <!-- <conditional-action>
            <if>
              <not>
                Not VA Agent group - test to see if this is necessary
                <number-compare test="equals">
                  <agent-group-id/>
                  <number>10007226</number>
                </number-compare>
              </not>
              <do>
                <set-boolean-var var-id="liveChat">
                  <true/>
                </set-boolean-var>
              </do>
            </if>
          </conditional-action>-->
          <conditional-action>
            <if>
              <or>
                <where>
                  <content-group-check cg-id="WNG_OLB"/>
                </where>
                <where>
                  <url-match regexp="(.*)wingsfinancial.architect-cert.fiservapps.com(.*)"/>
                </where>
              </or>
              <do>
                <send-data-to-agent>
                  <item name="Authenticated_User">
                    <as-string>
                      <string-variable-ref var-id="userName"/>
                    </as-string>
                  </item>
                </send-data-to-agent>
                <set-boolean-var var-id="NameSent">
                  <true/>
                </set-boolean-var>
                <log>
                  <string>Rule 1106: Data sent to agent</string>
                </log>
              </do>
            </if>
          </conditional-action>
        </actions>
      </rule>
      <rule id="1107" name="setName">
        <condition>
          <when>
            <event event-type="onPageLanding"/>
          </when>
          <as-boolean>
            <js-function-result>
              <anonymous-js-function>
                <![CDATA[
									function(){
                  try{if(document.getElementById("memberFullNameCap")){console.log("success"); return true;}}
                  catch(e){};
										return false;
									}
									]]>
              </anonymous-js-function>
            </js-function-result>
          </as-boolean>
        </condition>
        <actions>
          <set-string-var var-id="userName">
            <as-string>
              <js-function-result>
                <anonymous-js-function>
                  <![CDATA[
									function(){
                  var message="";
                  try{
                    var name = document.getElementById("memberFullNameCap").innerHTML;
                    console.log(name);
                    message += "<font color=\'#FF5733\'><b>"  + name + " has just logged in</b></font>";
                    return message;}
                  catch(e){};
										return "not_set";
									}
									]]>
                </anonymous-js-function>
              </js-function-result>
            </as-string>
          </set-string-var>
        </actions>
      </rule>
